#include <stdio.h>
int main(void){
    int i = 0;
    int f = 0;
    int g = 1;
    int x;
    int j = 0;
    printf("Welcome to the C program.\n");
    printf("Letâ€™s print out some numbers:\n");
    for (i = 0; i < 40; i++) {
        printf("%10d", g);
        x = g;
        g = g + f;
        f = x;
        j = j + 1;
        if(j == 5){
            printf("\n");
            j=0;
        }
    }
    printf("Finished.\n");
    return 0;
}
